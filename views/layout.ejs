<% const adminCounts = typeof adminActionCounts !== 'undefined' ? adminActionCounts : {}; %>
<div class="background-scene" aria-hidden="true">
  <div class="blob blob-1"></div>
  <div class="blob blob-2"></div>
  <div class="blob blob-3"></div>
</div>
<div class="app-shell">
  <% if (typeof showCookieBanner !== 'undefined' && showCookieBanner) { %>
    <% const policyUrl = typeof cookiePolicyUrl !== 'undefined' ? cookiePolicyUrl : '/cookies/politique'; %>
    <section
      class="cookie-banner glass-panel"
      data-cookie-banner
      role="region"
      aria-live="polite"
      aria-label="<%= t('layout.cookie.aria') %>"
    >
      <div class="cookie-banner__content">
        <h2><%= t('layout.cookie.title') %></h2>
        <p>
          <%= t('layout.cookie.text.beforeLink') %>
          <a href="<%= policyUrl %>"><%= t('layout.cookie.text.link') %></a>
          <%= t('layout.cookie.text.afterLink') %>
        </p>
      </div>
      <div class="cookie-banner__actions">
        <button class="btn primary" type="button" data-cookie-accept><%= t('layout.cookie.accept') %></button>
        <a class="btn secondary" href="<%= policyUrl %>" data-cookie-customize><%= t('layout.cookie.customize') %></a>
      </div>
    </section>
  <% } %>
  <header class="site-header glass-panel">
    <div class="header-start">
      <button
        id="sidebarToggle"
        class="nav-trigger icon-button"
        type="button"
        aria-label="<%= t('layout.nav.open') %>"
        aria-expanded="false"
      >
        <span class="icon" aria-hidden="true">‚ò∞</span>
        <span class="sr-only"><%= t('layout.nav.showNav') %></span>
      </button>
      <a class="brand-link" href="/">
