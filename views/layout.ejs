      <small><%- footerText %></small>
    <% } %>
    <% const policyUrl = typeof cookiePolicyUrl !== 'undefined' ? cookiePolicyUrl : '/cookies/politique'; %>
    <nav class="footer-links" aria-label="<%= t('layout.footer.aria') %>">
      <a href="<%= policyUrl %>"><%= t('cookiePolicy.title') %></a>
    </nav>
  </footer>
